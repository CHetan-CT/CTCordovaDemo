<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.fcm" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">

    <branch-config>
        <branch-key value="key_test_pgJYiEYvUOomm5yaQ30gVcdasxcIE2is" />
        <uri-scheme value="testcordova" />
        <link-domain value="i2f8.test-app.link" />
        <ios-team-release value="PW4Q8885U7" />
    </branch-config>

    <name>HelloCordova</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-build-tool" value="gradle" />

    <plugin name="clevertap-cordova" source="npm">
        <param name="CLEVERTAP_ACCOUNT_ID" value="" />
        <param name="CLEVERTAP_TOKEN" value="" />
    </plugin>

    <plugin name="branch-cordova-sdk" spec="file:../../tmp" />
    <engine name="ios" spec="^4.5.4" />
    <engine name="android" spec="^7.0.0" />
</widget>
